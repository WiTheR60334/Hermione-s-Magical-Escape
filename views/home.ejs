    let clickButton = document.querySelector('#click');
    clickButton.onclick = () => {
      console.log("Button Clicked!");
      dropdownMenu.classList.toggle('active');
    };
    function handleClickOutside(event) {
      if (!dropdownMenu.contains(event.target) && !clickButton.contains(event.target)) {
        dropdownMenu.classList.remove('active');
      }
    }
      document.addEventListener('click', handleClickOutside);
  </script>



<script>
    const cabinOptions = document.querySelectorAll('.cabinOption');
    const selectedOptionElement = document.querySelector('.selectedOption');
    let selectedOption = 'Business';
    selectedOptionElement.textContent = selectedOption;
  
    cabinOptions.forEach(option => {
      option.addEventListener('click', () => {
        cabinOptions.forEach((option) => {
          option.classList.remove('selected');
        });
        option.classList.add('selected');
        selectedOption = option.textContent;
        selectedOptionElement.textContent = selectedOption;
      });
    });
  
    const passengerOptions = document.querySelectorAll('.box .passengerOption');
  
    let totalPassengers = 1;
    clickElement.textContent = totalPassengers;
  
    passengerOptions.forEach(option => {
      option.addEventListener('click', () => {
        const optionText = option.textContent;
        if (optionText === '+') {
          totalPassengers = Math.min(totalPassengers + 1, 5);
        } else if (optionText === '-' && totalPassengers > 1) {
          totalPassengers -= 1;
        }
        clickElement.textContent = totalPassengers;
      });
    });
  </script>
  

  <script>
