<script>
    let wizard = document.getElementById('Wizard');
    let right = document.getElementById('Right');
    let left = document.getElementById('Left');
    let text1 = document.getElementById('text1');
    let text2 = document.getElementById('text2');

    function handleParallax() {
      let value = window.scrollY;
      background.style.left = value * .06 + 'px';
      wizard.style.left = -value * .3 + 'px';
      right.style.left = value * .3 + 'px';
      left.style.left = -value * .3 + 'px';
      text1.style.marginLeft = -value * 2.7 + 'px';
      text2.style.marginRight= -value * 2 + 'px';
    }

  
    function handleMobileParallax() {
      let value = window.scrollY;
      let wizard = document.getElementById('Wizard');
      let right = document.getElementById('Right');
      let left = document.getElementById('Left');
      let text1 = document.getElementById('text1');
      let text2 = document.getElementById('text2');
  
      wizard.style.left = -value * .05 + 'px';
      right.style.left = value * .13 + 'px';
      left.style.left = -value * .13 + 'px';
      text1.style.marginLeft = -value * 2.7 + 'px';
      text2.style.marginRight= -value * 2 + 'px';
    }
    
    window.addEventListener('scroll', function () {
      if (window.innerWidth <= 768) {
        handleMobileParallax();
      } else {
        handleParallax();
      }
    });
  </script>


<script>
    const adultCountElement = document.getElementById('adultCount');
    const adultPlusIcon = document.getElementById('adultPlus');
    const adultMinusIcon = document.getElementById('adultMinus');
    
    const childrenCountElement = document.getElementById('childrenCount');
    const childrenPlusIcon = document.getElementById('childrenPlus');
    const childrenMinusIcon = document.getElementById('childrenMinus');
    
    let adultCount = 1;
    let childrenCount = 0;
    
    function updateCount(countElement, value, min, max) {
      let count = parseInt(countElement.textContent) + value;
      count = Math.min(Math.max(min, count), max);
      countElement.textContent = count;
    }
    
    adultPlusIcon.addEventListener('click', () => {
      updateCount(adultCountElement, 1, 1, 5);
      adultCount = parseInt(adultCountElement.textContent);
      updateTotal();
    });
    
    adultMinusIcon.addEventListener('click', () => {
      updateCount(adultCountElement, -1, 1, 5);
      adultCount = parseInt(adultCountElement.textContent);
      updateTotal();
    });
    
    childrenPlusIcon.addEventListener('click', () => {
      updateCount(childrenCountElement, 1, 0, 5);
      childrenCount = parseInt(childrenCountElement.textContent);
      updateTotal();
    });
    
    childrenMinusIcon.addEventListener('click', () => {
      updateCount(childrenCountElement, -1, 0, 5);
      childrenCount = parseInt(childrenCountElement.textContent);
      updateTotal();
    });
    
    const clickElement = document.getElementById('click');
    
    function updateTotal() {
      const totalPassengers = adultCount + childrenCount;
      clickElement.textContent = totalPassengers;
    }
    
    updateTotal();
  </script>



  <script>
